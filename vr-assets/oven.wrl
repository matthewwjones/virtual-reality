##############################################################

Group 
{
	children
	[
		DEF	DialTime TimeSensor
		{
			cycleInterval 2.0
			loop FALSE	
			enabled	TRUE
		}

		DEF	DialInterpolatorOven OrientationInterpolator
		{
			key 		[0 0.5 1] 
			keyValue	[0 1 0 1.57, 0 1 0 0, 0 1 0 1.57]
		}
		DEF Heaterswitch Script 
		{
			url	"javascript:
			function set_onOff(onOff)
			{
				switchState=!(switchState)
				if(switchState) 
				{
					color_Changed[0]=1;	// R
					color_Changed[1]=0;	// G
					color_Changed[2]=0;	// B
				}
				else
				{
					color_Changed[0]=0; // R
					color_Changed[1]=0;	// G
					color_Changed[2]=0;	// B
				}
				//print('switchstate = ', switchState);
			}"
			eventIn	SFTime set_onOff
			eventOut SFColor color_Changed
			eventOut SFBool	switchState
			field SFBool switchState FALSE
		}

		DEF FanMover Script 
		{
			url	"javascript:
			function set_fraction(fraction,eventTime) 
			{
				//print('fantimer = ',eventTime);
				t=fraction;
				oneminust=1-t;
				value_changed[0]=0; // X Axis
				value_changed[1]=1;	// Y Axis
				value_changed[2]=0;	// Z Axis
				value_changed[3]=((oneminust*oneminust)*startx)+((t*t)*endx); // Rotation angle
			}"
			eventIn	SFFloat set_fraction
			eventOut SFRotation	value_changed
			field SFFloat startx 0.0
			field SFFloat endx 6.28
			field SFFloat t 0.0
		}

		DEF	FanSwitch Script
		{
			url	"javascript:
			function onOff(on_off)
			{
				toggle = (!toggle)
				if(toggle)
				{
					toggle=1;
				}
				else
				{
					toggle=0;
				}
			}"
			eventIn	SFTime onOff
			eventOut SFBool	toggle
			field SFBool toggle	FALSE
		}

		DEF FanMove OrientationInterpolator	
		{
			key			[0 1]
			keyValue	[0 0 1 0, 0 0 1 3.14, 0 0 1 0]
		}

		DEF FanTime TimeSensor 
		{
			cycleInterval 0.2
			loop TRUE
			enabled FALSE
		}
	]
}


